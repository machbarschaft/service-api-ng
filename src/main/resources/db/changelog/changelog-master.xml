<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="https://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!-- When binding to a different DBMS than PostgreSQL, you need to add the correct generation function for UUID-like values here -->
    <property name="uuid_function" value="uuid_generate_v4()" dbms="postgresql"/>

    <includeAll path="1.0.0" relativeToChangelogFile="true"/>

</databaseChangeLog>
